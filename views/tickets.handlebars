<!--dashboard start-->
<div class = "col-md-9 col-sm-12 col-xm-12" style = "margin-top:28px;">

             {{#if tRemoval}}
			     <div class = "alert alert-success" alert-dismissable style = "border-radius:4px;"> 
				     
					     <p><button type = "button" class = "close" data-dismiss = "alert" aria-hidden = "true"><i class = "lnr lnr-cross-circle"></i></button>
				         <i class= "lnr lnr-success"></i> Property removed successfully</p>
					 
				 </div> 
			 {{/if}}

		 <table class="table">
			  <thead>
				<tr>
				  <th scope="col">Cell #</th>
				  <th scope="col">Status</th>
				  <th scope="col">Action</th>
				</tr>
			  </thead>
			  <tbody>
			{{#each tickets}}
				<tr>
				  <th scope="row">{{this.clientCell}}</th>
				  <td><span class ="label {{this.myClass}}">{{this.status}}</span></td>
				  <td><a href = "/ticket-id/{{this._id}}"><span class ="label label-success">view</span></a></td>
				
				</tr>
			{{/each}}
			  </tbody>
		</table>
		 
</div>
     </div>        
</div>	  
	 



